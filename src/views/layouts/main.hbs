<html lang='en'>
  <head>
    <meta charset='UTF-8' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <title>Document</title>
    <link
      href='https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css'
      rel='stylesheet'
      integrity='sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH'
      crossorigin='anonymous'
    />
    <script src='https://accounts.google.com/gsi/client' async defer></script>
    <link
      rel='stylesheet'
      href='https://cdn.jsdelivr.net/npm/bootstrap-icons@1.3.0/font/bootstrap-icons.css'
    />
  </head>
  <body>
    <nav class='navbar navbar-expand-lg bg-primary fixed-top navbar-dark'>
      <!-- Container wrapper -->
      <div class='container-fluid'>
        <!-- Toggle button -->
        <button
          class='navbar-toggler'
          type='button'
          data-bs-toggle='collapse'
          data-bs-target='#navbarSupportedContent'
          aria-controls='navbarSupportedContent'
          aria-expanded='false'
          aria-label='Toggle navigation'
        >
          <i class='fas fa-bars'></i>
        </button>

        <!-- Collapsible wrapper -->
        <div class='collapse navbar-collapse'>
          <!-- Navbar brand -->
          <a class='navbar-brand mt-2 mt-lg-0' href='#'>
            <img
              src='/assets/logo.png'
              alt='MDB Logo'
              loading='lazy'
              style="width: 30px; object-fit:contain"

            />
          </a>
          <!-- Left links -->
          <ul class='navbar-nav me-auto mb-2 mb-lg-0'>
            <li class='nav-item'>
              <a class='nav-link' href='/'>Dashboard</a>
            </li>
            {{#if isAdmin}}
              <li class='nav-item'>
                <a class='nav-link' href='/member/all-members'>All Members</a>
              </li>
              <li class='nav-item'>
                <a class='nav-link' href='/brand/all-brands'>Brand</a>
              </li>
              <li class='nav-item'>
                <a class='nav-link' href='/watch/all-watches'>Watch</a>
              </li>
            {{/if}}
            
          </ul>
          <!-- Left links -->
        </div>
        <!-- Collapsible wrapper -->

        <!-- Right elements -->
        <div class='d-flex align-items-center'>
          <!-- Avatar -->
          {{#if user}}
            <div class='dropdown ms-3 row'>
              <h4 class="col" style="white-space: nowrap;">{{userObject.name}}</h4>
              <a
                class='col dropdown-toggle d-flex align-items-center hidden-arrow'
                href='#'
                role='button'
                aria-expanded='false'
                data-bs-toggle='dropdown'
              >
                
                <img
                  src='http://bootdey.com/img/Content/avatar/avatar1.png'
                  class='rounded'
                  height='40'
                  alt='Black and White Portrait of a Man'
                  loading='lazy'
                />
              </a>
              <ul class='dropdown-menu dropdown-menu-end'>
                <li>
                  <a class='dropdown-item' href='/member/profile'>My profile</a>
                </li>
                <li>
                  <a class='dropdown-item' href='/member/change-password'>Change password</a>
                </li>
                <li>
                  <a class='dropdown-item' href='/logout'>Logout</a>
                </li>
              </ul>
            </div>
          {{else}}
            <ul class='navbar-nav me-auto mb-2 mb-lg-0'>
              <li class='nav-item'>
                <a class='nav-link' href='/login'>Login</a>
              </li>
            </ul>
          {{/if}}

        </div>
        <!-- Right elements -->
      </div>
      <!-- Container wrapper -->
    </nav>
    <div style='height: 54px;'></div>
    {{{body}}}


    <script
      src='https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js'
      integrity='sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r'
      crossorigin='anonymous'
    ></script>
    <script
      src='https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.min.js'
      integrity='sha384-0pUGZvbkm6XF6gxjEnlmuGrJXVbNuzT9qBBavbLwCsOGabYfZo0T0to5eqruptLy'
      crossorigin='anonymous'
    ></script>
    <script>
      const tooltipTriggerList = document.querySelectorAll('[data-bs-toggle="tooltip"]')
      const tooltipList = [...tooltipTriggerList].map(tooltipTriggerEl => new bootstrap.Tooltip(tooltipTriggerEl))
    </script>
  </body>
</html>